var g_num : int = 0;

proc foo() => int {
    var r := g_num;

    g_num += 1;

    return r;
}

proc main() => int {
    var arr : [8]int;

    // The left-hand-side is only evaluated once. Therefore, arr[0] == 1 after the statement below.
    arr[foo()] += 1;

    // 1 + 1 + 2 + 3 + 4 = 11
    return arr[0] + foo() + foo() + foo() + foo();
}
