
@exported
proc len(s : ^char) => usize
{
    if (!s) {
        return 0;
    }

    var count : usize = 0;

    while (*s) {
        s = s + 1;
        count = count + 1;
    }

    return count;
}

@exported
const CSTR_CONST : int = 20;

@exported
var cstr_var : char = 100;

var _internal_var : int;
