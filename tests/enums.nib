enum TokenKind : s16 {
    TK_NONE,
    TK_INT,
    TK_IDENT = 10,
    TK_PLUS
}

#static_assert(#sizeof(#typeof(TK_INT)) == 2);

proc main() => int {
    #writeout("Enum\n", 5);

    var token_names : []^char = {
        [TK_INT] = "integer",
        [TK_PLUS] = "+"
    };

    #writeout(token_names[TK_INT], 7);
    #writeout("\n", 1);

    return TK_NONE + TK_INT + TK_IDENT + TK_PLUS + 8; // 0 + 1 + 10 + 11 + 8 = 30
}
