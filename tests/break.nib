
proc main() => int {
    var i := 0;
    var j := 0;
    var k := 0;

    // Infinite loop
    while (1) {
        i = i + 1;

        if (i == 10) {
            break;
        }
        if (i >= 10) {
            i = 0; // Should not execute
        }
    }

    // While loop
    while (j < 20) {
        j = j + 1;

        if (j == 10) {
            break;
        }

        if (j >= 10) {
            j = 0; // Should not execute
        }
    }

    // Do-while loop
    do {
        k = k + 1;

        if (k > 9) {
            break;
        }

        if (k >= 10) {
            k = 0; // Should not execute
        }
    } while (k < 20);

    // TODO: for loop

    return i + j + k; // 10 + 10 + 10 = 30
}
