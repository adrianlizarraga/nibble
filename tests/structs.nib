struct Lexer {
    x : char;
    id : int;
    str: ^char;
    at : ^char;
}

proc main() => int {
    #writeout("Structs\n", 8);

    var lexer : Lexer;
    lexer.id = 10;

    var p_lexer : ^Lexer = ^lexer;
    p_lexer.x = 5;

    // 10 + 5 + 5 + 5 + 100 = 125
    return lexer.id + p_lexer.x + lexer.x + (*p_lexer).x + 100;
}
