var g_s : [6]char = "HELLO";
var g_a : [4]char;

proc first_char(str: ^char) => char {
    return str[0];
}

proc main() => int
{
    var a : ^char = "Hello";

    //"Hello"[1] = 'Z'; // Segfaults because literals are stored in readonly memory.

    var s : [6]char = "12345";

    // Cannot initialize array after declaration.
    //var t : [6]char;
    //t = "12345";

    return first_char("Sailor\n`") + a[1] - s[4] + g_s[1] - g_a[2]; // 'S' + 'e' - '5' + 'E' - 0 <=> 83 + 101 - 53 + 69 - 0 = 200
}

