var g_a : [][2]int = {
    {0, 1}, {2, 3}
};

var g_b : [2][3]char = {
    "cc", {'d', 'd'}
};

var g_c : [2]^char = {
    "ee", "ff"
};

proc main() => int {
    var a : [2][2]int = {
        {0, 1},
        {2, 3}
    };

    var b : [2][3]char = {
        "aa",
        "bb"
    };

    var nl : char = '\n';

    #writeout(b[1], 2);
    #writeout("\n", 1);
    #writeout(g_b[0], 2);
    #writeout("\n", 1);
    #writeout(g_b[1], 2);
    #writeout("\n", 1);
    #writeout(g_c[1], 2);
    #writeout(^nl, 1);

    return a[1][1] + g_a[1][0]; // 3 + 2 = 5
}
