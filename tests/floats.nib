var g_f64 : f64 = 1.2;
var g_f32 : f32 = 1.2f;

proc main() => int {
    var l_f32 : f32 = 1.8f;
    var l_f64 : f64 = 1.2;
    var l2_f32 : f32 = l_f32 + 1.1f;
    var l2_f64 : f64 = l_f64 + 1.1;

    var l_int : int = 2;
    var l_s64 : s64 = 3;
    var l3_f32 : f32 = l_int :> f32;
    var l3_f64 : f64 = (l_s64 + 1) :> f64;

    var l_s8 : s8 = 2;
    var l2_s8 : s8 = 1;
    var l4_f64 : f64 = l_s8 :> f64;
    var l4_f32 : f32 = (l_s8 + l2_s8) :> f32;

    // 1 + 1 + 2 + 2 + 2 + 1 + 2 + 4 + 2 + 3 = 20
    return (l_f64 :> u64) + (l_f32 :> u32) + (l2_f32 :> u32) + (l2_f64 :> u32) + ((g_f64 + 1.3) :> u8) + ((g_f32 + 0.3f) :> u16) + 
           (l3_f32 :> int) + (l3_f64 :> int) + (l4_f64 :> int) + (l4_f32 :> int);
}
